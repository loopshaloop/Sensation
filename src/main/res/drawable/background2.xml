<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="70"
            android:toDegrees="70"
            android:pivotX="-10%"
            android:pivotY="90%" >
            <shape
                android:shape="rectangle" >
                <stroke android:color="#00000000"
                    android:width="10dp"/>
                <solid
                    android:color="#F67919" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="-85"
            android:toDegrees="-85"
            android:pivotX="5%"
            android:pivotY="10%" >
            <shape
                android:shape="rectangle" >
                <stroke android:color="#00000000"
                    android:width="10dp"/>
                <solid
                    android:color="#F67919" />
            </shape>
        </rotate>
    </item>
</layer-list>